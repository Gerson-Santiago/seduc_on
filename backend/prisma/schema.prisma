generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alunos_integracao_all {
  id                Int       @id @default(autoincrement())
  tipo_de_ensino    String?
  serie1            String?
  n_chamada         Int?
  nome_aluno        String?
  ra                String    @db.VarChar(20)
  dig               String?   @db.VarChar(5)
  uf                String?   @db.Char(2)
  data_nasci        DateTime? @db.Date
  data_inicial      DateTime? @db.Date
  data_fim          DateTime? @db.Date
  situacao          String?   @db.VarChar(10)
  data_movimentacao DateTime? @db.Date
  deficiencia       String?
  endereco          String?
  pos_censo         String?
  genero            String?   @db.Char(1)
  transporte        String?   @db.VarChar(20)
  etnia             String?   @db.VarChar(10)
  cod_turma         String?   @db.VarChar(20)
  periodo           String?   @db.VarChar(20)
  email_aluno       String?
  ra_senha          String?
  telefone          String?
  nome_responsavel  String?
  turma             String?
  filtro_serie      String?
  cod_escola        String?   @db.VarChar(20)
  nome_escola       String?
  inep              String?
  letra_turma       String?
  letra_turno       String?
  tipo_32           String?
  turma_escola      String?
  prof1             String?
  mais_info1        String?
  mais_info2        String?
  mais_info3        String?
  mais_info4        String?
  mais_info5        String?
  mais_info6        String?
  ra_uuid           String?   @db.Uuid

  @@map("alunos_integracao_all")
}

